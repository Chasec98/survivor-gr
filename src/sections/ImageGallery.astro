---
import { Image } from 'astro:assets';
import farmImg from '../assets/images/farm.avif';
---
<section class="bg-white relative overflow-hidden min-h-96 flex justify-center items-center">
    <Image src={farmImg} alt="farm" class="object-cover absolute inset-0 w-full h-full z-0 blur-sm scale-150" />
    <div class="relative flex flex-col items-center justify-center gap-4 max-w-[1024px] mx-auto p-6 z-10 text-white">
        <h5 class="font-chelsea text-primary text-[25px]">Image Gallery Under Construction</h5>
    </div>
</section>
